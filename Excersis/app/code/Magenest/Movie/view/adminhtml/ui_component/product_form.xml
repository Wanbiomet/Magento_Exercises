<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="course_info" sortOrder="1" >
        <settings>
            <collapsible>true</collapsible>
            <label>Course Info</label>
        </settings>
        <field name="course_start_time" formElement="date" component="Magenest_Movie/js/calendar-mixin">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="options" xsi:type="array">
                        <item name="showsTime" xsi:type="boolean">true</item>
                    </item>
                    <item name="source" xsi:type="string">start_date</item>
                </item>
            </argument>
            <settings>
                <validation>
                    <rule name="validate-date" xsi:type="boolean">true</rule>
                </validation>
                <dataType>timestamp</dataType>
                <label translate="true">Start Date</label>
                <visible>true</visible>
                <dataScope>start_date</dataScope>
            </settings>
        </field>
        <field name="course_end_time" formElement="date" component="Magenest_Movie/js/calendar-mixin">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
